#!/usr/bin/env zsh

## Main function set
# Class --Autosource

function \


    spin-ball() {
      revolver --style 'bouncingBall' start 'Generating'
      sleep 3
      revolver stop

   }

    spin-triangle() {
      revolver --style 'triangle' start 'Process Loading...'
      sleep 3
      revolver stop
   }

    spin2() {
      revolver --style 'balloon2' start 'Plese wait.  Task is generating'
      sleep 3
      revolver stop
   }

    spin3() {
      revolver --style 'balloon' start 'Generating needed files...'
      sleep 3
      revolver stop
   }

   spin4() {
      revolver --style 'circle' start 'Please wait. This might take a while'
      sleep 3
      revolver stop
   }

    spin5() {
      revolver --style 'shark' start 'Please Wait'
      sleep 3
      revolver stop
   }

    spin6() {
      revolver --style 'dots4' start 'Please Wait'
      sleep 3
      revolver stop
   }

    spin7() {
      revolver --style 'arc' start 'Please Wait'
      sleep 3
      revolver stop
   }

    spin8() {
      revolver --style 'arrow' start 'Please Wait'
      sleep 3
      revolver stop
   }

    spin9() {
      revolver --style 'hamburger' start 'Please Wait'
      sleep 3
      revolver stop
   }

    spin10() {
      revolver --style 'squish' start 'Generating'
      sleep 3
      revolver stop
   }

    spin11() {
      revolver --style 'dots12' start 'Generating'
      sleep 3
      revolver stop
   }

    spin12() {
      revolver --style 'flip' start
      sleep 5
      revolver stop
   }

    spin13() {
      revolver --style 'pong' start 'Hold up a sec..'
      sleep 3
      revolver stop
   }


    spin14() {
      revolver --style 'arrow2' start 'Please Wait'
      sleep 3
      revolver stop
   }

export sda2="/dev/sda2"
export sda3="/dev/sda3"
export sda4="/dev/sda4"
export sda1="/dev/sda1"
export sda2="/dev/sda2"
export sda3="/dev/sda3"
export sda4="/dev/sda4"

export sdb="/dev/sdb"

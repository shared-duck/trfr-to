# Exports for Installation

export ma="/master"
export m="/main"
export mm="/mnt/main"
export mma="/mnt/master"
export locals="/main/source-docs/locals"
export sodo="/main/source-docs"
export btkb="F4:73:35:6E:47:66"
export all="$sodoc/all"
export f="$m/functions"
export scr="$m/scripts"
export is="$?"
export this="$0"
export was="$$"
export zinit="$scr/zinit.sh"

export nv="/dev/nvme0n1"
export nv1="/dev/nvme0n1p1"
export nv2="/dev/nvme0n1p2"
export nv3="/dev/nvme0n1p3"
export nv4="/dev/nvme0n1p4"

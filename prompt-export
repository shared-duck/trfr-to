export x="/usr"
export xb="/usr/bin"
export xsb="/usr/sbin"
export xl="/usr/local"
export xs="/usr/share"
export xl="/usr/lib"

export b="/bin"
export sb="/sbin"
export va="/var"
export li="/lib"
export ma="/master"
export m="/main"

export safpa="$xl/sbin:$xl/bin:$xb:$xsb:$b:$sb"
export usafpa="$b:$sb:$va:$li:$ma:$m"
export perpa="$xb/site_perl:$xb/vendor_perl:$xb/core_perl"
export mepa="$HOME/bin:$HOME/.local/bin"
export mefupa="$fbin:$func"

export xenv="/usr/bin/env"
export func="$f"
export fsource="/main/source-docs/function-autosource"
export plugs="/main/scripts"
export theme="$HOME/bin/theme"
export ubin="$HOME/bin"
